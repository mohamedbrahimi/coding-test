{
  "name": "swagger-ui-nodejs",
  "version": "1.0.0",
  "description": "using swagger ui with nodejs ",
  "main": "server.js",
  "scripts": {
    "start_dev": "npx babel-node server.js",
    "start": "node build/server.js",
    "test": "mocha --require @babel/register  test/ --recursive --require @babel/polyfill",
    "clean": "rimraf build",
    "build": "npm run clean && babel ./ --out-dir build/ --ignore ./node_modules,./package.json,./npm-debug.log --copy-files",
    "build_start": "npm run-script build && npm run-script start"
  },
  "author": "Mophamed Aissa Brahimi",
  "license": "ISC",
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": "current"
          }
        }
      ]
    ],
    "plugins": [
      "@babel/plugin-syntax-dynamic-import",
      "@babel/plugin-syntax-import-meta",
      "@babel/plugin-proposal-class-properties",
      "@babel/plugin-proposal-json-strings",
      "@babel/plugin-proposal-optional-chaining"
    ]
  },
  "dependencies": {
    "bcrypt": "^5.0.0",
    "compression": "^1.7.4",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "jwt-simple": "^0.5.6",
    "mongoose": "^5.10.8",
    "swagger-jsdoc": "^3.4.0",
    "swagger-ui-express": "^4.1.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.7",
    "@babel/core": "^7.7.7",
    "@babel/node": "^7.7.7",
    "@babel/plugin-proposal-class-properties": "^7.7.4",
    "@babel/plugin-proposal-export-default-from": "^7.10.4",
    "@babel/plugin-proposal-json-strings": "^7.7.4",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.8.3",
    "@babel/plugin-proposal-optional-chaining": "^7.7.5",
    "@babel/plugin-syntax-dynamic-import": "^7.7.4",
    "@babel/plugin-syntax-import-meta": "^7.7.4",
    "@babel/plugin-syntax-jsx": "^7.10.4",
    "@babel/polyfill": "^7.8.7",
    "@babel/preset-env": "^7.7.7",
    "@babel/register": "^7.9.0",
    "add": "^2.0.6",
    "babel-eslint": "^10.0.3",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-airbnb-flow": "^1.0.2",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.17.0"
  }
}
